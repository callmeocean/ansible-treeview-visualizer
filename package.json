{
	"name": "ansible-treeview-visualizer",
	"displayName": "Ansible Treeview Visualizer",
	"description": "Visualizing ansible playbook with treeview, showing roles and facts that are used",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:*/*.yml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "ansible",
					"title": "Ansible",
					"icon": "./src/media/ansible_sm.png"
				}
			]
		},
		"views": {
			"ansible": [
				{
					"id": "ansibleTreeView",
					"name": "Ansible Treeview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"guid-typescript": "^1.0.9"
	}
}
